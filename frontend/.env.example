# Backend API URL
VITE_API_URL=http://localhost:5000/api

# Google Maps API Key (get from Google Cloud Console)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# ImageKit Configuration (for frontend direct uploads if needed)
VITE_IMAGEKIT_PUBLIC_KEY=your_imagekit_public_key
VITE_IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_imagekit_id

# Optional: frontend secure storage key (defense-in-depth only)
# If omitted, an ephemeral per-session key will be generated in the browser.
# Provide a long random string (>=32 chars) if you want deterministic keying.
VITE_SECURE_STORAGE_KEY=
